    "plugins": ["jest", "react"],
    "ignorePatterns": [
        "node_modules/",
        "dist/",
        "*.config.js",
        "setupTests.ts"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
